                        swiperAnimateCache(this); //隐藏动画元素
                        swiperAnimate(this); //初始化完成开始动画

                        bindEvent();
                        swiperAnimateMulCache(this); //隐藏动画元素
                        swiperAnimateMul(this); //初始化完成开始动画
                    },
                    slideChangeTransitionStart: function () {
                        //console.log(this.activeIndex + '-00');//切换开始时，记录现在是第几个slide
                        //if (this.activeIndex == 1) {
                        //    restartAnim.seek(0);
                        //}
                        //console.log(this.activeIndex + '-0');//切换开始时，记录现在是第几个slide
                    },
                    slideChangeTransitionEnd: function () {
                        swiperAnimate(this); //每个slide切换结束时也运行当前slide动画
                        //this.slides.eq(this.activeIndex).find('.ani').removeClass('ani'); //动画只展现一次，去除ani类名

                        swiperAnimateMul(this); //初始化完成开始动画
                        //this.slides.eq(this.activeIndex).find('.ani-mul').removeClass('ani-mul'); //动画只展现一次，去除ani-mul类名

                        //console.log(this.activeIndex + '-1');//切换结束时，记录现在是第几个slide

                        if (this.activeIndex == 1) { // 切换到activeIndex等于1页
                            restartAnim.restart();
                        } else {
                            restartAnim.seek(0);
                        }

                        //console.log(this.activeIndex + '-2');//切换结束时，记录现在是第几个slide
                    }
                }
            })
        }

        function bgmToggle() {
            var audio = document.getElementById('bgm-audio');
            //var play = document.getElementById('play');
            //var pause = document.getElementById('pause');
            $('#audio-contorl').toggleClass("play");
            $('#audio-contorl').toggleClass("pause");
            if (audio !== null) {
                //检测播放是否已暂停.audio.paused 在播放器播放时返回false.
                //if (audio.paused) {
                if ($('#audio-contorl').hasClass("play")) {
                    audio.play();// 这个就是播放
                    //play.style.display = 'none';
                    //pause.style.display = 'block';
                } else {
                    audio.pause();// 这个就是暂停
                    //play.style.display = 'block';
                    //pause.style.display = 'none';
                }
            }
        }

